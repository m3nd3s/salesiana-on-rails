<h1>Editar</h1>

<div class="content-fluid">
  <div class="row-fluid">
    <div class="span12">
      <%= form_for @aluno do |f| %>
        <div class="row-fluid">
          <%= f.label :nome %>
          <%= f.text_field :nome, class: 'input-block-level' %>
        </div>

        <%= f.label :email %>
        <%= f.text_field :email %>

        <p><%= f.submit "Salvar" %></p>
      <% end %>
    </div>
  </div>
</div>
