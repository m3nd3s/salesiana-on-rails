<h1>Aqui vem a lista de alunos</h1>

<table border="1" width="100%">
  <tr>
    <th>Nome</th>
    <th>E-mail</th>
    <th>-</th>
  </tr>

  <% @alunos.each do |aluno| %>
    <td><%= aluno.nome %></td>
    <td><%= aluno.email %></td>
    <td>
      <%= link_to "Editar", edit_aluno_path(aluno) %>
    </td>
  <% end %>

</table>
